<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block css %}{% endblock %}
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
        <script>
            WebFont.load({
                google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
                active: function() {
                    sessionStorage.fonts = true;
                }
            });
        </script>
        <link rel="stylesheet" href="{{ asset('asset/css/vendors.bundle.css') }}">
        <link rel="stylesheet" href="{{ asset('asset/css/style.bundle.css') }}">
    </head>
    <body>
        {% block nav %}
            <header>
                <nav>
                    <ul>
                        <li><a href="{{ path('form') }}">Formularz</a></li>
                        <li><a href="{{ path('login') }}">Zaloguj siÄ™</a></li>
                    </ul>
                </nav>
            </header>
        {% endblock %}
        {% block body %}{% endblock %}

        <scritp src="{{ asset('asset/js/vendors.bundle.js') }}"></scritp>
        <scritp src="{{ asset('asset/js/scripts.bundle.js') }}"></scritp>

        {% block js %}{% endblock %}
    </body>
</html>
